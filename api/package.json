{
  "name": "workforce-portal-api",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "build:production": "npm run prestart && npm prune --production",
    "buildAndTest": "npm run build && npm run test && npm prune --production",
    "watch": "tsc --w",
    "prestart": "npm run build && func extensions install",
    "start:host": "func start",
    "start": "npm-run-all --parallel start:host watch",
    "test": "tsc && jest --passWithNoTests --roots ./tests",
    "test:integration": "env-cmd -f ./tests-integration/.env jest --roots ./tests-integration",
    "test:exploratory": "jest --roots ./tests-exploratory"
  },
  "description": "",
  "devDependencies": {
    "@azure/functions": "^3.5.1",
    "@eslint/js": "^9.2.0",
    "@microsoft/microsoft-graph-types": "^2.40.0",
    "@types/jest": "^29.5.12",
    "@types/uuid": "^8.3.4",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "envalid": "^8.0.0",
    "eslint": "^8.57.0",
    "globals": "^15.2.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0"
  },
  "dependencies": {
    "@aaronpowell/static-web-apps-api-auth": "^0.1.3",
    "@azure/identity": "^2.0.2",
    "@azure/msal-node": "^2.9.2",
    "@effect/platform": "^0.58.12",
    "@effect/platform-node": "^0.53.11",
    "@effect/schema": "^0.68.11",
    "@microsoft/microsoft-graph-client": "^3.0.1",
    "@pnp/common-commonjs": "^2.11.0",
    "@pnp/logging-commonjs": "^2.11.0",
    "@pnp/nodejs-commonjs": "^2.11.0",
    "@pnp/sp-commonjs": "^2.11.0",
    "effect": "^3.4.4",
    "fast-check": "^3.17.1",
    "formdata-node": "^6.0.3",
    "isomorphic-fetch": "^3.0.0",
    "uuid": "^8.3.2"
  }
}
